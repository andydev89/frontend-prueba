{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "build:theme": "postcss src/assets/vendor/theme/style.css -o src/assets/vendor/theme/style.scoped.css --config postcss.theme.cjs",
    "watch:theme": "postcss -w src/assets/vendor/theme/style.css -o src/assets/vendor/theme/style.scoped.css --config postcss.theme.cjs"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bootstrap": "5.3",
    "bootstrap-icons": "^1.13.1",
    "photoswipe": "^5.4.4",
    "pinia": "^3.0.3",
    "swiper": "^12.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-prefix-selector": "^2.1.1",
    "sass": "^1.93.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.1.0"
  }
}
